            bindings = <&kp LEFT_SHIFT>;
            key-positions = <16 17>;
            layers = <0 1 2 3>;
            timeout-ms = <50>;
        };

        combo_shift_right {
            bindings = <&kp RIGHT_SHIFT>;
            key-positions = <18 19>;
            timeout-ms = <50>;
            layers = <0 1 2 3>;
        };
    };

