            compatible = "zmk,behavior-hold-tap";
            label = "HML Mod AltGr";
            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <200>;
            quick-tap-ms = <200>;
            require-prior-idle-ms = <125>;
            hold-trigger-on-release;
            hold-trigger-key-positions = <MY_LEFT>;
            bindings = <&mod_layer>, <&agrtap>;
